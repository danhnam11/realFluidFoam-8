FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1;

boundaryField
{
    periodic1
    {
        type            cyclic;
        neighbourPatch  periodic2;
    }

    periodic2
    {   
        type            cyclic;
        neighbourPatch  periodic1;
    }

    empty
    {
        type            empty;
    }
}

